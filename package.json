{
  "name": "double-vue",
  "version": "0.2.3",
  "description": "The vue part of double",
  "type": "module",
  "scripts": {
    "build": "npm run build-cjs && parcel build && node ./postBuildCleanup.js",
    "build-cjs": "tsc -p tsconfig-cjs.json",
    "watch-cjs": "tsc --watch -p tsconfig-cjs.json",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/cjs/index.js",
    "./server": "./dist/cjs/server.js",
    "./pinia": "./dist/cjs/pinia.js",
    "./nuxt": "./dist/cjs/nuxt.js"
  },
  "files": [
    "src/**/*",
    "dist/index.d.ts",
    "dist/index.d.ts.map",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sopamo/double-vue.git"
  },
  "keywords": [
    "double",
    "vue",
    "laravel",
    "api",
    "vite",
    "nuxt",
    "pinia"
  ],
  "author": "Paul Mohr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sopamo/double-vue/issues"
  },
  "homepage": "https://github.com/Sopamo/double-vue#readme",
  "dependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "php-parser": "^3.0.3",
    "vue-demi": "^0.12.1"
  },
  "peerDependencies": {
    "@nuxt/kit-edge": "^3.0.0-27373619.4728fd5",
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "@nuxt/kit-edge": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nuxt/kit-edge": "^3.0.0-27373619.4728fd5",
    "@parcel/packager-ts": "^2.2.1",
    "@parcel/transformer-typescript-tsc": "^2.4.0",
    "@parcel/transformer-typescript-types": "^2.2.1",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "parcel": "^2.2.0",
    "path-browserify": "^1.0.1",
    "pinia": "^2.0.9",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "tty-browserify": "^0.0.1",
    "typescript": "^4.5.4",
    "unbuild": "^0.6.7",
    "unplugin": "^0.3.1",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "vite": "^2.7.11",
    "vitest": "^0.9.2",
    "vm-browserify": "^1.1.2",
    "vue": "^3.0.0"
  }
}
